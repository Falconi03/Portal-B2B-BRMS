  "version": "1.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean",
    "test": "jest -u --config ./jest.config.js --passWithNoTests",
    "lint": "yarn scss-lint && eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "yarn scss-lint:fix && eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "storybook": "start-storybook -p 6006 -c ./.storybook",
    "coverage": "jest --coverage",
    "test:watch": "cross-env NODE_ENV=debug yarn test --watch",
    "start:watch": "cross-env NODE_ENV=debug webpack --progress --colors --watch --config webpack.config.js",
    "start": "cross-env NODE_ENV=debug webpack-dev-server --progress --colors --watch --config webpack.config.js",
    "start:dev": "cross-env NODE_ENV=debug LOCAL_API=false webpack-dev-server --progress --colors --watch --config webpack.config.js",
    "build": "cross-env NODE_ENV=local webpack --config webpack.config.js",
    "homolog-build": "cross-env NODE_ENV=homolog webpack --config webpack.hom.js",
    "production-build": "cross-env NODE_ENV=production webpack --config webpack.prd.js",
    "build-storybook:screenshot": "SCREENSHOT=true build-storybook",
    "loki": "loki --reactUri file:./storybook-static",
    "loki:update": "loki update --reactUri file:./storybook-static",
    "loki:ci": "loki update --chromeFlags='--headless --disable-gpu --hide-scrollbars --no-sandbox --no-first-run'",
    "regression": "reg-suit run",
    "prettier:fix": "prettier --write './**/*.{scss,json,js,jsx}' --config ../.prettierrc",
    "prettier": "prettier --list-different './**/*.{scss,json,js,jsx}' --config ../.prettierrc",
    "scss-lint": "sass-lint ./**/**/*.scss -v -q --config ../.sass-lint.yml",
    "scss-lint:fix": "sass-lint-auto-fix --config ../.sass-lint.yml",
    "backend-checks": "bash ../../pre-push.sh py",
    "pre-push": "bash ../../pre-push.sh",
    "typecheck": "tsc",
    "cy": "yarn cypress open",
    "cy:run": "yarn cypress run",
    "cy:run:ci": "yarn cypress run --config video=true"
  },
